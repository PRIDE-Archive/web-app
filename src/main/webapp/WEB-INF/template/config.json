{
    "globalTemplate": {
        "title": "EMBL-EBI PRIDE Archive",
        "javascriptList": [
            {
                "url": "viewer/viewer.nocache.js"
            }
        ],
        "lastInBody": true,
        "cssList": [
            {
                "url": "static/css/webapp.css"
            }
        ],
        "placeHolder": "webapp",
        "bodyMaxWidth": 1050
    },
    "localMasthead": {
        "style": "logo-title",
        "title": "PRIDE Archive",
        "homepageUrl": "//www.ebi.ac.uk/pride/archive",
        "logo": {
            "src": "//www.ebi.ac.uk/pride/resources/logos/pride_logo.jpg",
            "alt": "logo",
            "width": "64",
            "height": "64"
        },
        "search": {
            "btnText": "Search",
            "action": "../simpleSearch",
            "method": "get",
            "inputName": "q",
            "examples": [
                {
                    "title": "stress",
                    "href": "../simpleSearch?q=stress"
                },
                {
                    "title": "human",
                    "href": "../simpleSearch?q=human"
                },
                {
                    "title": "blood",
                    "href": "../simpleSearch?q=blood"
                },
                {
                    "title": "P02768",
                    "href": "../simpleSearch?q=P02768"
                },
                {
                    "title": "MDPNTIIEALR*",
                    "href": "../simpleSearch?q=MDPNTIIEALR*"
                }
            ]
        },
        "navigationTabs": [
            {
                "selected": false,
                "title": "Home",
                "href": "../",
                "tooltip": "Home page"
            },
            {
                "selected": false,
                "title": "Search",
                "href": "../search",
                "tooltip": "Search PRIDE Archive data"
            },
            {
                "selected": false,
                "title": "Documentation",
                "href": "../documentation",
                "tooltip": "PRIDE Archive documentation"
            },
            {
                "selected": false,
                "title": "About PRIDE Archive",
                "href": "../about",
                "tooltip": "About PRIDE Archive"
            }
        ],
        "functionalTabs": [
            {
                "title": "Feedback",
                "href": "/support",
                "iconFontSet": "icon-static",
                "dataIcon": "\\"
            }
        ]
    }
}